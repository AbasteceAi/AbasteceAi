<legend>SearchCounty</legend>

<%= form_tag("/searcher", method: "get") do %>
	<h2 id = "title-form">Pesquisar Município</h2>
		<div id="state-group">
		<%= label_tag(:state_searched, "Estado") %>
		<%= text_field_tag 'state_searched', nil, placeholder: "Nome do Estado", :required => true %>
	</div>
 	<div id="county-group">
		<%= label_tag(:county_searched, "Município") %>
		<%= text_field_tag 'county_searched', nil, placeholder: "Nome do Município", :required => true %>
	</div>
	<%= submit_tag("Pesquisar", class: "btn btn-success") %>
<% end %>

<p> 
	<%= @counties_of_state %>
</p>

<p>
	<%= @last_research%>
</p>

<% @teste.each do |fuel| %>
<p>
<%= fuel.fuel_type.type_name %>
<%= fuel.max_resale_price %></p>
<% end %>

